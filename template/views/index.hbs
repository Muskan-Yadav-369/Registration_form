<html lang="en">
<head>
  <meta charset="utf-8">
  <meta name="viewport" content="width=device-width, initial-scale=1">
  <title>Student Registration Form</title>
  <link href="https://cdn.jsdelivr.net/npm/bootstrap@5.3.2/dist/css/bootstrap.min.css" rel="stylesheet" integrity="sha384-T3c6CoIi6uLrA9TneNEoa7RxnatzjcDSCmG1MXxSR1GAsXEV/Dwwykc2MPK8M2HN" crossorigin="anonymous">
  <style>
    /* Custom styles */
    body {
      background-image: url('https://images.unsplash.com/photo-1605627079912-97c3810a11a4?w=500&auto=format&fit=crop&q=60&ixlib=rb-4.0.3&ixid=M3wxMjA3fDB8MHxzZWFyY2h8MTV8fGNoaWxkcmVuJTIwc3R1ZHlpbmd8ZW58MHx8MHx8fDA%3D'); /* Replace 'path/to/your/image.jpg' with the actual path to your background image */
      background-size: cover;
      background-position: center;
      background-repeat: no-repeat;
      color: #ffffff; /* Text color for better contrast with the background image */
      font-family: 'Arial', sans-serif;
      margin: 0;
      padding: 0;
    }
    .logo-container {
      position: fixed;
      top: 20px;
      left: 20px;
      z-index: 1; /* Ensure the logo appears above the background image */
    }
    .logo {
      width: 200px; /* Adjust the width as needed */
      height: 200px;
    }
    .container {
      background-color: rgba(0, 0, 0, 0.5); /* Set a semi-transparent black background for the form container */
      padding: 20px;
      border-radius: 10px;
      box-shadow: 0 0 10px rgba(255, 255, 255, 0.5); /* Add a white box shadow for better visibility */
      margin-top: 80px; /* Adjust the margin to create space for the fixed logo */
    }
    .form-label {
      font-weight: bold;
    }
    .form-control {
      color: #000000; /* Text color for the form fields */
      background-color: #ffffff; /* Background color for the form fields */
    }
    /* Add more custom styles as needed */
  </style>
</head>
<body>



  <div class="logo-container">
    <img src="data:image/png;base64," alt="Anokhi Pehel Logo" class="logo"> <!-- Replace 'path/to/your/logo.png' with the actual path to your logo -->
  </div>



  <div class="container mt-5 shadow-lg p-5" style="width:55%">
   
    <h2 class="text-center"> Student Registration Form</h2>





    <form action="/register" method="POST">
      <div class="mb-3">
        <label for="exampleInputName" class="form-label">Student Name</label>
        <input
          type="text"
          name="name"
          class="form-control"
          id="exampleInputName"
          required
        />
      </div>

      <div class="mb-3">
        <label for="exampleInputFatherName" class="form-label">Father's Name</label>
        <input
          type="text"
          name="fatherName"
          class="form-control"
          id="exampleInputFatherName"
          required
        />
      </div>

      <div class="mb-3">
        <label for="exampleInputClass" class="form-label">Class</label>
        <input
          type="text"
          name="class"
          class="form-control"
          id="exampleInputClass"
          required
        />
      </div>

      <div class="mb-3">
        <label for="exampleInputAge" class="form-label">Age</label>
        <input
          type="number"
          name="age"
          class="form-control"
          id="exampleInputAge"
          required
        />
      </div>

      <div class="mb-3">
        <label for="exampleInputPhone" class="form-label">Phone</label>
        <input
          type="number"
          name="phone"
          class="form-control"
          id="exampleInputPhone"
          required
        />
      </div>

      <div class="mb-3">
        <label for="exampleInputLocation" class="form-label">Location</label>
        <input
          type="text"
          name="location"
          class="form-control"
          id="exampleInputLocation"
          required
        />
      </div>

      <button type="submit" class="btn btn-primary">Submit</button>
    </form>
  </div>

  <script src="https://cdn.jsdelivr.net/npm/bootstrap@5.3.2/dist/js/bootstrap.bundle.min.js" integrity="sha384-C6RzsynM9kWDrMNeT87bh95OGNyZPhcTNXj1NW7RuBCsyN/o0jlpcV8Qyq46cDfL" crossorigin="anonymous"></script>
</body>
</html>
